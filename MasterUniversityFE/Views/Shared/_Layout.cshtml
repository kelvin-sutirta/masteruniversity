<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MasterUniversityFE</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MasterUniversityFE.styles.css" asp-append-version="true" />
    <!--<script src-"http://code.jquery.com/jquery-1.11.3.min.js"></script>-->
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">MasterUniversityFE</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item sqldropdown"> SQL
                         <ul class="isi-sqldropdown">
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="SQL" asp-action="SQL">Insert</a>
                        </li>
                         <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="SQL" asp-action="SQLUpdate">Update</a>
                        </li>                       
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="SQL" asp-action="SQLGet">Get</a>
                        </li>                       
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="SQL" asp-action="SQLDelete">Delete</a>
                        </li>
                            </ul>
                        </li>
                         <li class="nav-item nosqldropdown">NoSQL
                         <ul class="isi-nosqldropdown">
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="NoSQL" asp-action="NoSQL">Insert</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="NoSQL" asp-action="NoSQLUpdate">Update</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="NoSQL" asp-action="NoSQLGet">Get</a>
                        </li>
                         <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="NoSQL" asp-action="NoSQLDelete">Delete</a>
                        </li>

                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - MasterUniversityFE
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
